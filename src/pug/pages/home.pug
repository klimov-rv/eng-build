					.swiper-slide
						include products/st11
					.swiper-slide
						include products/st02
					.swiper-slide
						include products/stAll_wmd06_bh06
				.slider-cube-nav
					.container
						.row
							.push-1.cell-10.post-1.push-1-sm.cell-10-sm.post-1-sm.push-0-xs.cell-12-xs.post-0-xs.slider-cube-relative
								.swiper-button-prev
								.swiper-button-next 
						.row.hide.show-md
							.swiper-pagination
