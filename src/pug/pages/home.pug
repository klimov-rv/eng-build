	section.advantages#advantages 
		.container.reserve-height__container.text-center
			//- Преимущества
			.reserve-height 
				span.lang(translate="advantages") Преимущества 
				h2.advantages__header.text-pinned.lang#js-dynamic-text-2(translate="advantages")
		.container.text-center 
			.advantages__icons 
				.icons_wrap
					.icons_list.advan.row
						each object, key in data.home.advanages.icons
							- langclass = "" 
							if (temp_lang == 'en')
								- langclass = "eng-icon"
							if (temp_lang == 'sg')
								- langclass = "eng-icon"
							if (temp_lang == 'fr')
								- langclass = "fr-icon"
							.icon__item.cell-2.cell-4-xs(class='advantages__icon-' + key + ' '+langclass)
								.col.flex-middle(data-aos-delay=object.delay data-aos="fade-in" data-aos-anchor-placement="top-bottom")
									figure.icon(
										class=object.svg
									)
									span.icon__signature.lang(translate=object.translate)
										!= object.text
	section.two.gray-bg#products
